class=3Dcode-comment>//</SPAN><SPAN class=3Dcode-comment> returns the =
next Node of the path
</SPAN>        <SPAN class=3Dcode-keyword>public</SPAN> <SPAN =
class=3Dcode-keyword>int</SPAN> ProbablePath(ArrayList VisitedNodes)
        {
            <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment> create a random generator
</SPAN>            Random r =3D <SPAN class=3Dcode-keyword>new</SPAN> =
Random(Global.Seed);

            <SPAN class=3Dcode-keyword>double</SPAN> val=3D0;
            <SPAN class=3Dcode-keyword>double</SPAN> count =3D <SPAN =
class=3Dcode-digit>0</SPAN>;
            <SPAN class=3Dcode-keyword>double</SPAN> Lastcount =3D =
-<SPAN class=3Dcode-digit>1</SPAN>;
   =20
            ArrayList tempTEVector=3Dnew ArrayList();

            <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment> loop through all the connected nodes
</SPAN>            <SPAN class=3Dcode-keyword>for</SPAN>(<SPAN =
class=3Dcode-keyword>int</SPAN> i=3D0;i<SPAN =
class=3Dcode-keyword>&lt;</SPAN>tableEntry.Length;i++)
            {
                <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment> has the node been visitied?
</SPAN>                <SPAN class=3Dcode-keyword>bool</SPAN> v=3Dfalse;

                <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment>loop through all the visited nodes
</SPAN>                <SPAN class=3Dcode-keyword>for</SPAN>(<SPAN =
class=3Dcode-keyword>int</SPAN> j=3D0;j<SPAN =
class=3Dcode-keyword>&lt;</SPAN>VisitedNodes.Count;j++)
                {
                    <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment> if the ID's match then this node has alrady been =
visited
</SPAN>                    <SPAN =
class=3Dcode-keyword>if</SPAN>(tableEntry[i].NodeID=3D=3D(<SPAN =
class=3Dcode-keyword>int</SPAN>)VisitedNodes[j])
                        v=3Dtrue;
                }
               =20
                <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment> If v is false then the node hasnt been visited.. =
so Add
</SPAN>                <SPAN class=3Dcode-keyword>if</SPAN>(!v)=20
                {
                    <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment> get the node
</SPAN>                    Node n =3D =
Global.Nodes[tableEntry[i].NodeID];

                    <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment> if the node is accepting connections
</SPAN>                    <SPAN =
class=3Dcode-keyword>if</SPAN>(!n.FullCapacity)
                    {
                        <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment> add the node as a possible candidate
</SPAN>                        tempTEVector.Add(tableEntry[i]);
                    }
                }
            }
           =20
            <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment> if all connections have been visited
</SPAN>            <SPAN =
class=3Dcode-keyword>if</SPAN>(tempTEVector.Count=3D=3D0)
            {
                <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment> loop through all the connected nodes
</SPAN>                <SPAN class=3Dcode-keyword>for</SPAN>(<SPAN =
class=3Dcode-keyword>int</SPAN> i=3D0;i<SPAN =
class=3Dcode-keyword>&lt;</SPAN>tableEntry.Length;i++)
                    tempTEVector.Add(tableEntry[i]);
            }

            <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment> get the ceiling amount for probabilities
</SPAN>            <SPAN class=3Dcode-keyword>for</SPAN>(<SPAN =
class=3Dcode-keyword>int</SPAN> i=3D0;i<SPAN =
class=3Dcode-keyword>&lt;</SPAN>tempTEVector.Count;i++)
                val+=3D ((TableEntry)tempTEVector[i]).Probablilty;

            <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment>create randon value
</SPAN>            val =3D r.NextDouble()*val;

            <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment> loop through the temp Table Entryies
</SPAN>            <SPAN class=3Dcode-keyword>for</SPAN>(<SPAN =
