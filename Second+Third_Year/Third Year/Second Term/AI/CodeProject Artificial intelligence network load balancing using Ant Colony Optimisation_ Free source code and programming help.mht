        }




</PRE>Return the next node via the pheromone table<BR><PRE>        <SPAN =
class=3Dcode-comment>//</SPAN><SPAN class=3Dcode-comment> updates the =
probabilities of the pheromone table by multiplying the selected=20
</SPAN>        <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment> nodes probability by a radio of  newVal
</SPAN>        <SPAN class=3Dcode-keyword>public</SPAN> <SPAN =
class=3Dcode-keyword>void</SPAN> UpdateProbabilities(<SPAN =
class=3Dcode-keyword>double</SPAN> newVal, <SPAN =
class=3Dcode-keyword>int</SPAN> EntryTableNodeID)
        {
            TableEntry t;
            <SPAN class=3Dcode-keyword>double</SPAN> total=3D0;

            <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment> loop through all the table entries
</SPAN>            <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment> get the total enumeration of probabilities and add =
the new value.
</SPAN>            <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment> Since this total will be more than 100 a ratio =
multiplication is
</SPAN>            <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment> applied. Although these values will not equate to =
exactly 100% floating
</SPAN>            <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment> point calculations will be accurate enough at =
least 99.99999% which is satisfactory
</SPAN>            <SPAN class=3Dcode-keyword>for</SPAN>(<SPAN =
class=3Dcode-keyword>int</SPAN> j=3D0;j<SPAN =
class=3Dcode-keyword>&lt;</SPAN>tableEntry.Length;j++)
            {
                t =3D tableEntry[j];

                <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment> enumerate the total probablility
</SPAN>                total +=3D t.Probablilty;
               =20
                <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment> if the table entry matches the id of the chosen =
node path
</SPAN>                <SPAN =
class=3Dcode-keyword>if</SPAN>(EntryTableNodeID=3D=3Dt.NodeID)
                {
                    <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment> add the new value to the total
</SPAN>                    total +=3D newVal;
                    t =3D tableEntry[j];
                    <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment> add the new value the current value of the =
selected path
</SPAN>                    t.Probablilty +=3D newVal;
                }
            }

            <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment> calculate the ratio for the multiplcation
</SPAN>            <SPAN class=3Dcode-keyword>double</SPAN> ratio =3D =
100/total;

            <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment> loop through each table entry and multiple the =
current probability
</SPAN>            <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment> by the new ratio
</SPAN>            <SPAN class=3Dcode-keyword>for</SPAN>(<SPAN =
class=3Dcode-keyword>int</SPAN> j=3D0;j<SPAN =
class=3Dcode-keyword>&lt;</SPAN>tableEntry.Length;j++)
            {
                tableEntry[j].Probablilty *=3D ratio;
            }

            <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment> this will enumerate all the values to 99.99999%
</SPAN>        }

        <SPAN class=3Dcode-comment>//</SPAN><SPAN class=3Dcode-comment> =
Constructor takes a node to represent and a list of all connected nodes =
off the
</SPAN>        <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment> calling node
</SPAN>        <SPAN class=3Dcode-keyword>public</SPAN> =
PheromoneTable(Node n, <SPAN class=3Dcode-keyword>int</SPAN>[] conns)
        {
            <SPAN class=3Dcode-keyword>this</SPAN>.NodeID =3D n.ID;
       =20
            <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment> create a tableEntry array the same length as the =
number of connections
</SPAN>            <SPAN class=3Dcode-keyword>this</SPAN>.tableEntry =3D =
<SPAN class=3Dcode-keyword>new</SPAN> TableEntry[conns.Length];

            <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment> create a new tableEntry for each connection
</SPAN>            <SPAN class=3Dcode-keyword>for</SPAN>(<SPAN =
class=3Dcode-keyword>int</SPAN> i=3D0;i<SPAN =
class=3Dcode-keyword>&lt;</SPAN>conns.Length;i++)
                tableEntry[i] =3D <SPAN class=3Dcode-keyword>new</SPAN> =
TableEntry(conns[i]);

            <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment> set default equal values
</SPAN>            <SPAN class=3Dcode-keyword>for</SPAN>(<SPAN =
class=3Dcode-keyword>int</SPAN> i=3D0;i<SPAN =
class=3Dcode-keyword>&lt;</SPAN>conns.Length;i++)
                tableEntry[i].Probablilty =3D (<SPAN =
class=3Dcode-digit>100</SPAN> / (<SPAN =
class=3Dcode-keyword>double</SPAN>)conns.Length);
        }
    }


