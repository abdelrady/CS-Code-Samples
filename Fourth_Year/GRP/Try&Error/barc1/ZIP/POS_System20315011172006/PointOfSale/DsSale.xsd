<?xml version="1.0" standalone="yes"?>
<xs:schema id="DsSale" targetNamespace="http://www.tempuri.org/DsSale.xsd" xmlns:mstns="http://www.tempuri.org/DsSale.xsd" xmlns="http://www.tempuri.org/DsSale.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="qualified" elementFormDefault="qualified">
  <xs:element name="DsSale" msdata:IsDataSet="true">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element name="SaleByItem">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Date" type="xs:dateTime" minOccurs="0" />
              <xs:element name="Description" type="xs:string" minOccurs="0" />
              <xs:element name="ItemCode" type="xs:string" />
              <xs:element name="Qty" type="xs:int" minOccurs="0" />
              <xs:element name="Total" type="xs:decimal" minOccurs="0" />
              <xs:element name="UnitPrice" type="xs:decimal" minOccurs="0" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
    <xs:unique name="Constraint1" msdata:PrimaryKey="true">
      <xs:selector xpath=".//mstns:SaleByItem" />
      <xs:field xpath="mstns:ItemCode" />
    </xs:unique>
  </xs:element>
</xs:schema>