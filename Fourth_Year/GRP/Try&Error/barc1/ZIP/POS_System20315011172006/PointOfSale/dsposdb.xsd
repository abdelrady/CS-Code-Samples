<?xml version="1.0" standalone="yes"?>
<xs:schema id="dsposdb" targetNamespace="http://www.tempuri.org/dsposdb.xsd" xmlns:mstns="http://www.tempuri.org/dsposdb.xsd" xmlns="http://www.tempuri.org/dsposdb.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="qualified" elementFormDefault="qualified">
  <xs:element name="dsposdb" msdata:IsDataSet="true">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element name="Login">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Password" type="xs:string" minOccurs="0" />
              <xs:element name="Username" type="xs:string" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="Item_x0020_Master">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Barcode" type="xs:string" minOccurs="0" />
              <xs:element name="Description" type="xs:string" minOccurs="0" />
              <xs:element name="ItemCode" type="xs:string" />
              <xs:element name="MinSellingPrice" type="xs:decimal" minOccurs="0" />
              <xs:element name="RetailPrice" type="xs:decimal" minOccurs="0" />
              <xs:element name="StandardCost" type="xs:decimal" minOccurs="0" />
              <xs:element name="StockQty" type="xs:int" minOccurs="0" />
              <xs:element name="StockReorderLevel" type="xs:int" minOccurs="0" />
              <xs:element name="Unit_x002F_Measure" type="xs:string" minOccurs="0" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
    <xs:unique name="Constraint1" msdata:PrimaryKey="true">
      <xs:selector xpath=".//mstns:Login" />
      <xs:field xpath="mstns:Username" />
    </xs:unique>
    <xs:unique name="Item_x0020_Master_Constraint1" msdata:ConstraintName="Constraint1" msdata:PrimaryKey="true">
      <xs:selector xpath=".//mstns:Item_x0020_Master" />
      <xs:field xpath="mstns:ItemCode" />
    </xs:unique>
  </xs:element>
</xs:schema>