            {
            // TODO: write error handler
            }=20
            TextOut(hdc, 2, 55, szBuf, cch);=20
            break;=20
=20
        default:=20
            break;=20
    }=20
=20
    ReleaseDC(hwndMain, hdc);=20
    return CallNextHookEx(myhookdata[DEBUG].hhook, nCode, wParam,=20
        lParam);=20
}=20
=20
/****************************************************************=20
