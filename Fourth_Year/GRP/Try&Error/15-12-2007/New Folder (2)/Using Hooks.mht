=20
    if (nCode &lt; 0)  // do not process message=20
        return CallNextHookEx(myhookdata[DEBUG].hhook, nCode,=20
            wParam, lParam);=20
=20
    hdc =3D GetDC(hwndMain);=20
=20
    switch (nCode)=20
    {=20
        case HC_ACTION:
            hResult =3D StringCchPrintf(szBuf, 128/sizeof(TCHAR),  =20
                "DEBUG - nCode: %d, tsk: %ld, %d times   ",=20
                nCode,wParam, c++);
            if (FAILED(hResult))
            {
            // TODO: write error handler
            }
            hResult =3D StringCchLength(szBuf, 128/sizeof(TCHAR), =
