  </SPAN>Encoding.GetEncoding(1252).GetBytes(lcPostData);<o:p></o:p></P>
  <P class=3DCodeListing>loHttp.ContentLength =3D=20
lbPostBuffer.Length;<o:p></o:p></P>
  <P class=3DCodeListing><o:p>&nbsp;</o:p></P>
  <P class=3DCodeListing>Stream loPostData =3D=20
  loHttp.GetRequestStream();<o:p></o:p></P>
  <P=20
  =
class=3DCodeListing>loPostData.Write(lbPostBuffer,0,lbPostBuffer.Length);=
<o:p></o:p></P>
  <P class=3DCodeListing>loPostData.Close();<o:p></o:p></P>
  <P class=3DCodeListing><o:p>&nbsp;</o:p></P>
