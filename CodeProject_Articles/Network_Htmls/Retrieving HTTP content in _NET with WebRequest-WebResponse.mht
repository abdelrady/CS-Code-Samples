  </SPAN>oGetUrls.ParentForm =3D this;</P>
  <P class=3DCodeListing><SPAN style=3D"mso-spacerun: yes">&nbsp;&nbsp; =
</SPAN></P>
  <P class=3DCodeListing><SPAN style=3D"mso-spacerun: yes">&nbsp;&nbsp;=20
  </SPAN>ThreadStart oDelegate =3D </P>
  <P class=3DCodeListing><SPAN=20
  style=3D"mso-spacerun: =
yes">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&n=
bsp;&nbsp;&nbsp;=20
