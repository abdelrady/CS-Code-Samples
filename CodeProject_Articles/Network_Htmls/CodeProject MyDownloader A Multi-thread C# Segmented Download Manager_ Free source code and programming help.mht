class=3Dcode-keyword>string</SPAN> pageData,=20
         <SPAN class=3Dcode-keyword>out</SPAN> <SPAN =
class=3Dcode-keyword>string</SPAN> videoTitle)
   {
      videoTitle =3D TextUtil.JustAfter(pageData,
          <SPAN class=3Dcode-string>"</SPAN><SPAN =
class=3Dcode-string>&lt; meta name=3D\"title\" content=3D\""</SPAN>, =
<SPAN class=3Dcode-string>"</SPAN><SPAN =
class=3Dcode-string>\"&gt;"</SPAN>);=20
     =20
      <SPAN class=3Dcode-keyword>return</SPAN> =
ResourceLocation.FromURL(<SPAN =
class=3Dcode-SDKkeyword>String</SPAN>.Format(<SPAN =
class=3Dcode-string>"</SPAN><SPAN =
class=3Dcode-string>{0}/get_video?video_id=3D{1}&amp;t=3D{2}"</SPAN>,=20
       TextUtil.GetDomain(url), TextUtil.JustAfter(url, <SPAN =
class=3Dcode-string>"</SPAN><SPAN class=3Dcode-string>v=3D"</SPAN>, =
<SPAN class=3Dcode-string>"</SPAN><SPAN =
class=3Dcode-string>&amp;"</SPAN>),=20
       TextUtil.JustAfter(pageData, <SPAN =
class=3Dcode-string>"</SPAN><SPAN class=3Dcode-string>&amp;t=3D"</SPAN>, =
<SPAN class=3Dcode-string>"</SPAN><SPAN =
class=3Dcode-string>&amp;hl=3D"</SPAN>)));
