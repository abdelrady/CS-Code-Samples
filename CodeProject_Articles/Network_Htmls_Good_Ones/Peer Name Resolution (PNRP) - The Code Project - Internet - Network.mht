      {
         scope =3D Scope;
         Reset();
      }

     ~PeerCloudEnumerator()
     {
        <SPAN class=3Dcs-keyword>if</SPAN> (pResults !=3D IntPtr.Zero) =
Marshal.FreeHGlobal(pResults);
        <SPAN class=3Dcs-keyword>if</SPAN> (hLookup !=3D IntPtr.Zero) =
PnrpNative.WSALookupServiceEnd(hLookup);
     }

<SPAN class=3Dcs-preprocessor>#region IEnumerator Members</SPAN>

