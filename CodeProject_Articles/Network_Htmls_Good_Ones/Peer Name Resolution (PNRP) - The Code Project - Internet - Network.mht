{
    <SPAN class=3Dcs-keyword>int</SPAN> length =3D <SPAN =
class=3Dcs-literal>255</SPAN>;
    System.Text.StringBuilder DnsName =3D=20
                   <SPAN class=3Dcs-keyword>new</SPAN> =
System.Text.StringBuilder(length);
    <SPAN class=3Dcs-keyword>int</SPAN> err =3D =
PnrpNative.PeerNameToPeerHostName(PnrpName,=20
                                    DnsName, <SPAN =
class=3Dcs-keyword>ref</SPAN> length);
    <SPAN class=3Dcs-keyword>if</SPAN> (err !=3D <SPAN =
class=3Dcs-literal>0</SPAN>)
        <SPAN class=3Dcs-keyword>throw</SPAN> <SPAN =
class=3Dcs-keyword>new</SPAN> System.Net.Sockets.SocketException(err);
    <SPAN class=3Dcs-keyword>return</SPAN> DnsName.ToString();
}

<SPAN class=3Dcs-keyword>public</SPAN> <SPAN =
