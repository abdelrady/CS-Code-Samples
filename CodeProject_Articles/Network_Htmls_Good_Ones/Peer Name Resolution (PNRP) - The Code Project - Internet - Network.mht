       =20
        querySet.dwSize =3D Marshal.SizeOf(<SPAN =
class=3Dcs-keyword>typeof</SPAN>(WSAQUERYSET));
        querySet.dwNameSpace =3D <SPAN class=3Dcs-literal>39</SPAN>; =
<SPAN class=3Dcs-comment>// NS_PNRPCLOUD</SPAN>
