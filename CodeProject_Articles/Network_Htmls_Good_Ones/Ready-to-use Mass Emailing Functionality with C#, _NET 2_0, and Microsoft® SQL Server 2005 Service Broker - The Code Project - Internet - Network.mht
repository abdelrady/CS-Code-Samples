  <SPAN class=3Dcs-keyword>if</SPAN> (<SPAN =
class=3Dcs-keyword>null</SPAN> !=3D =
HttpContext.Current.Cache[DummyCacheItemKey])
      <SPAN class=3Dcs-keyword>return</SPAN>;

  HttpContext.Current.Cache.Add(DummyCacheItemKey,=20
              <SPAN class=3Dcpp-string>"Test"</SPAN>, <SPAN =
class=3Dcs-keyword>null</SPAN>, DateTime.MaxValue,
              TimeSpan.FromMinutes(<SPAN class=3Dcs-literal>1</SPAN>),=20
              CacheItemPriority.NotRemovable,
