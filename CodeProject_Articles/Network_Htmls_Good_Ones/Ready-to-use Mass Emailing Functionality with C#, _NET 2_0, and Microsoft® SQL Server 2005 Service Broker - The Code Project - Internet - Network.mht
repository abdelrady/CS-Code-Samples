<SPAN class=3Dcs-keyword>public</SPAN> <SPAN =
class=3Dcs-keyword>void</SPAN> CacheItemRemovedCallback( <SPAN =
class=3Dcs-keyword>string</SPAN> key, <SPAN =
class=3Dcs-keyword>object</SPAN> value,
                                      CacheItemRemovedReason reason)
{
  System.Diagnostics.Debug.WriteLine(<SPAN class=3Dcpp-string>"Cache =
item callback: "</SPAN> +=20
