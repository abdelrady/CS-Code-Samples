class=3Dcode-keyword>return</SPAN> (m_hFileMap !=3D NULL);}

    <SPAN class=3Dcode-keyword>bool</SPAN> ReadIPCMMF(LPBYTE pBuf, DWORD =
&amp;dwBufSize);
    <SPAN class=3Dcode-keyword>bool</SPAN> WriteIPCMMF(<SPAN =
class=3Dcode-keyword>const</SPAN> LPBYTE pBuf, <SPAN =
class=3Dcode-keyword>const</SPAN> DWORD dwBufSize);

    <SPAN class=3Dcode-keyword>bool</SPAN> Lock(<SPAN =
class=3Dcode-keyword>void</SPAN>);
    <SPAN class=3Dcode-keyword>void</SPAN> Unlock(<SPAN =
class=3Dcode-keyword>void</SPAN>);

