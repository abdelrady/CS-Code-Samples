class=3Dcode-comment>hook INT 3
</SPAN>        add ecx,<SPAN class=3Dcode-digit>16</SPAN>
        mov ebx,handler2

        mov word ptr[ecx],bx
        shr ebx,<SPAN class=3Dcode-digit>16</SPAN>
        mov word ptr[ecx+6],bx

