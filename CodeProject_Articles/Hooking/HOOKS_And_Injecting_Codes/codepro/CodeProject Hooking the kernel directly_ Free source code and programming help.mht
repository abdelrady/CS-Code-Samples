{
    ULONG handler1,handler2,idtbase,tempidt,a;
    UCHAR idtr[<SPAN class=3Dcode-digit>8</SPAN>];

    <SPAN class=3Dcode-comment>//</SPAN><SPAN class=3Dcode-comment>get =
the addresses that we have write to IDT
</SPAN>    handler1=3D(ULONG)&amp;replacementbuff[<SPAN =
class=3Dcode-digit>0</SPAN>];
    handler2=3D(ULONG)&amp;replacementbuff[<SPAN =
class=3Dcode-digit>32</SPAN>];

    <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment>allocate temp. memory. This should be our first=20
</SPAN>    <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment>step - from the moment we disable interrupts
</SPAN>    <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment>till return we don't risk to call any code=20
</SPAN>    <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment>that has not been written by ourselves
</SPAN>    <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment>(theoretically this code may re-enable=20
</SPAN>    <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment>interrupts without our knowledge, and then.....)
</SPAN>    tempidt=3D(ULONG)ExAllocatePool(NonPagedPool,<SPAN =
class=3Dcode-digit>2048</SPAN>);

    _asm
    {
        cli
       =20
        sidt idtr
        lea ebx,idtr
        mov eax,dword ptr[ebx+2]
        mov idtbase,eax
    }

    <SPAN class=3Dcode-comment>//</SPAN><SPAN class=3Dcode-comment>check =
whether our IDT has already been hooked.=20
</SPAN>    <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment>If yes, re-enable interrupts and return
</SPAN>    <SPAN class=3Dcode-keyword>for</SPAN>(a=3D0;a<SPAN =
class=3Dcode-keyword>&lt;</SPAN>IdtsHooked;a++)
    {
        <SPAN class=3Dcode-keyword>if</SPAN>(idtbases[a]=3D=3Didtbase)
        {
            _asm sti
            ExFreePool((<SPAN =
class=3Dcode-keyword>void</SPAN>*)tempidt);
            KeSetEvent(&amp;event,<SPAN =
class=3Dcode-digit>0</SPAN>,<SPAN class=3Dcode-digit>0</SPAN>);
            PsTerminateSystemThread(<SPAN class=3Dcode-digit>0</SPAN>);
        }
    }

    _asm
    {
        <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment>now we are going to load the copy of IDT into IDTR =
register
</SPAN>        <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment> in my experience, modifying memory,
</SPAN>        <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment>pointed to by IDTR register, is unsafe
</SPAN>        mov edi,tempidt
        mov esi,idtbase
        mov ecx,<SPAN class=3Dcode-digit>2048</SPAN>
        rep movs

        lea ebx,idtr
        mov eax,tempidt
        mov dword ptr[ebx+2],eax
        lidt idtr

        <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment>now we can safely modify IDT. Get ready
</SPAN>        mov ecx,idtbase

        <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment>hook INT 1
