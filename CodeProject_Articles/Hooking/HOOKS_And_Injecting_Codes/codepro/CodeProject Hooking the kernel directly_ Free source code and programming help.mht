_declspec(<SPAN class=3Dcode-keyword>naked</SPAN>) INT1Proxy()
{
    _asm   =20
    {
        pushfd
        pushad
        mov ebx,esp
        add ebx,<SPAN class=3Dcode-digit>36</SPAN>
        push ebx
        call INT1check

        cmp eax,<SPAN class=3Dcode-digit>0</SPAN>
        je fin

        popad
        popfd
        iretd

