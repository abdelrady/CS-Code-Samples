var_2C =3D byte=20
      ptr -2Ch<BR>.text:79F05ECA var_28 =3D dword ptr =
-28h<BR>.text:79F05ECA=20
      var_1C =3D byte ptr -1Ch<BR>.text:79F05ECA var_18 =3D dword ptr=20
      -18h<BR>.text:79F05ECA var_14 =3D dword ptr -14h<BR>.text:79F05ECA =
var_4 =3D=20
      dword ptr -4<BR>.text:79F05ECA<BR>.text:79F05ECA ; FUNCTION CHUNK =
AT=20
      .text:79FBF47D SIZE 0000005A BYTES<BR>.text:79F05ECA ; FUNCTION =
CHUNK AT=20
      .text:79FBF4FC SIZE 00000042 =
BYTES<BR>.text:79F05ECA<BR>.text:79F05ECA=20
      push 20h<BR>.text:79F05ECC mov eax, offset =
loc_7A2EE124<BR>.text:79F05ED1=20
