VOID FixReloc(VOID *pBase, IMAGE_NT_HEADERS *pNtHeaders)
{
    <SPAN class=3Dcode-comment>//</SPAN>
    <SPAN class=3Dcode-comment>//</SPAN><SPAN class=3Dcode-comment> Set =
first section to writeable in order to fix</SPAN>
    <SPAN class=3Dcode-comment>//</SPAN><SPAN class=3Dcode-comment> the =
relocations in the code</SPAN>
    <SPAN class=3Dcode-comment>//</SPAN>

    IMAGE_SECTION_HEADER *pCodeSect =3D (IMAGE_SECTION_HEADER *)
        IMAGE_FIRST_SECTION(pNtHeaders);

    VOID *pCode =3D (VOID *) (pCodeSect-<SPAN =
class=3Dcode-keyword>&gt;</SPAN>VirtualAddress + (ULONG_PTR) pBase);

    DWORD dwOldCodeProtect;

    VirtualProtect(pCode,
        pCodeSect-<SPAN =
class=3Dcode-keyword>&gt;</SPAN>Misc.VirtualSize,
        PAGE_READWRITE,
        &amp;dwOldCodeProtect);

    <SPAN class=3Dcode-comment>//</SPAN>
    <SPAN class=3Dcode-comment>//</SPAN><SPAN class=3Dcode-comment> =
Relocate</SPAN>
    <SPAN class=3Dcode-comment>//</SPAN>

    DWORD Delta =3D (DWORD)(((ULONG_PTR) pBase) -=20
        pNtHeaders-<SPAN =
class=3Dcode-keyword>&gt;</SPAN>OptionalHeader.ImageBase);

    DWORD RelocRva;

    <SPAN class=3Dcode-keyword>if</SPAN> (!(RelocRva =3D =
pNtHeaders-<SPAN =
class=3Dcode-keyword>&gt;</SPAN>OptionalHeader.DataDirectory
        [IMAGE_DIRECTORY_ENTRY_BASERELOC].VirtualAddress))
        <SPAN class=3Dcode-keyword>return</SPAN>;

    IMAGE_BASE_RELOCATION *ImgBaseReloc =3D=20
        (IMAGE_BASE_RELOCATION *) (RelocRva + (ULONG_PTR) pBase);

    WORD *wData;

    <SPAN class=3Dcode-keyword>do</SPAN>
    {
        <SPAN class=3Dcode-keyword>if</SPAN> (!ImgBaseReloc-<SPAN =
class=3Dcode-keyword>&gt;</SPAN>SizeOfBlock)
            <SPAN class=3Dcode-keyword>break</SPAN>;

        UINT nItems =3D (ImgBaseReloc-<SPAN =
class=3Dcode-keyword>&gt;</SPAN>SizeOfBlock -
            IMAGE_SIZEOF_BASE_RELOCATION) / <SPAN =
class=3Dcode-keyword>sizeof</SPAN> (WORD);

        wData =3D (WORD *)(IMAGE_SIZEOF_BASE_RELOCATION +
            (ULONG_PTR) ImgBaseReloc);

        <SPAN class=3Dcode-keyword>for</SPAN> (UINT i =3D <SPAN =
class=3Dcode-digit>0</SPAN>; i <SPAN class=3Dcode-keyword>&lt;</SPAN> =
nItems; i++)
        {
            DWORD Offset =3D (*wData &amp; 0xFFF) + ImgBaseReloc-<SPAN =
class=3Dcode-keyword>&gt;</SPAN>VirtualAddress;

            DWORD Type =3D *wData <SPAN =
class=3Dcode-keyword>&gt;</SPAN><SPAN class=3Dcode-keyword>&gt;</SPAN> =
<SPAN class=3Dcode-digit>12</SPAN>;

            <SPAN class=3Dcode-keyword>if</SPAN> (Type !=3D =
IMAGE_REL_BASED_ABSOLUTE)
            {
                DWORD *pBlock =3D (DWORD *)(Offset + (ULONG_PTR) pBase);

                *pBlock +=3D Delta;
            }

            wData++;
        }

        ImgBaseReloc =3D (PIMAGE_BASE_RELOCATION) wData;

    } <SPAN class=3Dcode-keyword>while</SPAN> (*(DWORD *) wData);

    <SPAN class=3Dcode-comment>//</SPAN>
    <SPAN class=3Dcode-comment>//</SPAN><SPAN class=3Dcode-comment> =
Restore memory settings</SPAN>
    <SPAN class=3Dcode-comment>//</SPAN>

    VirtualProtect(pCode,
        pCodeSect-<SPAN =
class=3Dcode-keyword>&gt;</SPAN>Misc.VirtualSize,
        dwOldCodeProtect,
        &amp;dwOldCodeProtect);
}</PRE>
