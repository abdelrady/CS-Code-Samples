VOID FixIAT(VOID *pBase, IMAGE_NT_HEADERS *pNtHeaders)
{
    <SPAN class=3Dcode-keyword>if</SPAN> (pNtHeaders-<SPAN =
class=3Dcode-keyword>&gt;</SPAN>OptionalHeader.DataDirectory
        [IMAGE_DIRECTORY_ENTRY_IMPORT].VirtualAddress =3D=3D <SPAN =
class=3Dcode-digit>0</SPAN>)
        <SPAN class=3Dcode-keyword>return</SPAN>;

    IMAGE_IMPORT_DESCRIPTOR *pImpDescr =3D (IMAGE_IMPORT_DESCRIPTOR *)
        (pNtHeaders-<SPAN =
class=3Dcode-keyword>&gt;</SPAN>OptionalHeader.DataDirectory
        [IMAGE_DIRECTORY_ENTRY_IMPORT].VirtualAddress +
        (ULONG_PTR) pBase);

    DWORD dwOldIATProtect;
    VOID *pIAT =3D NULL;

    <SPAN class=3Dcode-keyword>if</SPAN> (pNtHeaders-<SPAN =
class=3Dcode-keyword>&gt;</SPAN>OptionalHeader.DataDirectory
        [IMAGE_DIRECTORY_ENTRY_IAT].VirtualAddress !=3D <SPAN =
class=3Dcode-digit>0</SPAN>)
    {
        VOID *pIAT =3D (VOID *) (pNtHeaders-<SPAN =
class=3Dcode-keyword>&gt;</SPAN>OptionalHeader.DataDirectory
            [IMAGE_DIRECTORY_ENTRY_IAT].VirtualAddress +
            (ULONG_PTR) pBase);

        VirtualProtect(pIAT,
            pNtHeaders-<SPAN =
class=3Dcode-keyword>&gt;</SPAN>OptionalHeader.DataDirectory
            [IMAGE_DIRECTORY_ENTRY_IAT].Size,
            PAGE_EXECUTE_READWRITE,
            &amp;dwOldIATProtect);
    }

    <SPAN class=3Dcode-keyword>while</SPAN> (pImpDescr-<SPAN =
class=3Dcode-keyword>&gt;</SPAN>Name !=3D <SPAN =
class=3Dcode-digit>0</SPAN>)
    {
        <SPAN class=3Dcode-keyword>char</SPAN> *DllName =3D (<SPAN =
class=3Dcode-keyword>char</SPAN> *) (pImpDescr-<SPAN =
class=3Dcode-keyword>&gt;</SPAN>Name +
            (ULONG_PTR) pBase);

        HMODULE hImpDll =3D LoadLibraryA(DllName);

        <SPAN class=3Dcode-keyword>if</SPAN> (hImpDll =3D=3D NULL) <SPAN =
class=3Dcode-keyword>continue</SPAN>;

        THUNK *pThunk;

        <SPAN class=3Dcode-keyword>if</SPAN> (pImpDescr-<SPAN =
class=3Dcode-keyword>&gt;</SPAN>OriginalFirstThunk)
            pThunk =3D (THUNK *)(pImpDescr-<SPAN =
class=3Dcode-keyword>&gt;</SPAN>OriginalFirstThunk +
            (ULONG_PTR) pBase);
        <SPAN class=3Dcode-keyword>else</SPAN>
            pThunk =3D (THUNK *)(pImpDescr-<SPAN =
class=3Dcode-keyword>&gt;</SPAN>FirstThunk +
            (ULONG_PTR) pBase);

        THUNK *pIATThunk =3D (THUNK *) (pImpDescr-<SPAN =
class=3Dcode-keyword>&gt;</SPAN>FirstThunk +
            (ULONG_PTR) pBase);

        <SPAN class=3Dcode-keyword>while</SPAN> (*pThunk)
        {
            <SPAN class=3Dcode-keyword>if</SPAN> (IS_FLAG(*pThunk, =
IMAGE_ORDINAL_FLAG))
            {
                *pIATThunk =3D (THUNK) GetProcAddress(hImpDll,
                    (LPCSTR) (*pThunk ^ IMAGE_ORDINAL_FLAG));
            }
            <SPAN class=3Dcode-keyword>else</SPAN>
            {
                <SPAN class=3Dcode-keyword>char</SPAN> *pImpFunc =3D =
(<SPAN class=3Dcode-keyword>char</SPAN> *) (<SPAN =
class=3Dcode-keyword>sizeof</SPAN> (WORD) + ((ULONG_PTR) *pThunk) +
                    ((ULONG_PTR) pBase));

                *pIATThunk =3D (THUNK) GetProcAddress(hImpDll, =
pImpFunc);
            }

            pThunk++;
            pIATThunk++;
        }

        pImpDescr++;
    }

    <SPAN class=3Dcode-keyword>if</SPAN> (pIAT)
    {
        VirtualProtect(pIAT,
            pNtHeaders-<SPAN =
class=3Dcode-keyword>&gt;</SPAN>OptionalHeader.DataDirectory
            [IMAGE_DIRECTORY_ENTRY_IAT].Size,
            dwOldIATProtect,
            &amp;dwOldIATProtect);
    }
}

