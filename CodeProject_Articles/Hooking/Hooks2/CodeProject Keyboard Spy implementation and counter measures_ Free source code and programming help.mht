    srand((<SPAN class=3Dcode-keyword>unsigned</SPAN>)::GetTickCount());
    <SPAN class=3Dcode-keyword>int</SPAN> iKeyStrokeCount =3D rand() % =
<SPAN class=3Dcode-digit>5</SPAN> + <SPAN class=3Dcode-digit>1</SPAN>;

    <SPAN class=3Dcode-keyword>int</SPAN> key;
