    inp[<SPAN class=3Dcode-digit>0</SPAN>].ki.dwExtraInfo =3D =
::GetMessageExtraInfo();
    inp[<SPAN class=3Dcode-digit>0</SPAN>].ki.dwFlags =3D <SPAN =
class=3Dcode-digit>0</SPAN>;
    inp[<SPAN class=3Dcode-digit>0</SPAN>].ki.time =3D <SPAN =
class=3Dcode-digit>0</SPAN>;

    <SPAN class=3Dcode-keyword>for</SPAN> (<SPAN =
class=3Dcode-keyword>int</SPAN> i=3D0; i <SPAN =
class=3Dcode-keyword>&lt;</SPAN> iKeyStrokeCount; i++)=20
    {
        key =3D rand() % (<SPAN class=3Dcode-string>'</SPAN><SPAN =
class=3Dcode-string>Z'</SPAN>-<SPAN class=3Dcode-string>'</SPAN><SPAN =
class=3Dcode-string>A'</SPAN>) + <SPAN class=3Dcode-string>'</SPAN><SPAN =
class=3Dcode-string>A'</SPAN>;
        inp[<SPAN class=3Dcode-digit>0</SPAN>].ki.wScan =3D key;
        inp[<SPAN class=3Dcode-digit>0</SPAN>].ki.wVk =3D key;

        inp[<SPAN class=3Dcode-digit>1</SPAN>] =3D inp[<SPAN =
class=3Dcode-digit>0</SPAN>];
