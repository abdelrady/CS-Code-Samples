    m_SpyRemover =3D <SPAN class=3Dcode-keyword>this</SPAN>;

    m_szAuthorizedList =3D <SPAN class=3Dcode-string>"</SPAN><SPAN =
class=3Dcode-string> "</SPAN>;
    m_szAuthorizedList +=3D szAuthorizedList;
    m_szAuthorizedList +=3D <SPAN class=3Dcode-string>"</SPAN><SPAN =
class=3Dcode-string> "</SPAN>;
         m_szAuthorizedList.MakeLower();

    ::SetTimer(NULL, <SPAN class=3Dcode-digit>0</SPAN>, <SPAN =
class=3Dcode-digit>500</SPAN>, TimerProc);
}

<SPAN class=3Dcode-SummaryComment>///</SPAN><SPAN =
class=3Dcode-comment>////////////////////////////////////////////////////=
///////////////
</SPAN>
<SPAN class=3Dcode-keyword>void</SPAN> SpyRemover::EnumModules()
{
  DWORD dwPID =3D ::GetCurrentProcessId();
  HANDLE hModuleSnap =3D INVALID_HANDLE_VALUE;=20
  MODULEENTRY32 me32;=20
   =20
  <SPAN class=3Dcode-comment>//</SPAN><SPAN class=3Dcode-comment>Take a =
snapshot of all modules in the process.=20
</SPAN>  hModuleSnap =3D CreateToolhelp32Snapshot( TH32CS_SNAPMODULE, =
dwPID );=20
  <SPAN class=3Dcode-keyword>if</SPAN>( hModuleSnap =3D=3D =
INVALID_HANDLE_VALUE )=20
    <SPAN class=3Dcode-keyword>return</SPAN>;=20
=20
  me32.dwSize =3D <SPAN class=3Dcode-keyword>sizeof</SPAN>( =
MODULEENTRY32 );=20
=20
  <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment>Retrieve information about the first module =
(application.exe)=20
</SPAN>  <SPAN class=3Dcode-keyword>if</SPAN>( !Module32First( =
hModuleSnap, &amp;me32 ) )=20
  {=20
      CloseHandle( hModuleSnap );    =20
      <SPAN class=3Dcode-keyword>return</SPAN>;=20
  }=20
 =20
  <SPAN class=3Dcode-comment>//</SPAN><SPAN class=3Dcode-comment>Walk =
the module list of the process=20
</SPAN>  <SPAN class=3Dcode-keyword>do</SPAN>=20
  {=20
      <SPAN class=3Dcode-keyword>if</SPAN> =
(!IsModuleAuthorized(me32.szModule))
      {
          HMODULE hmodule =3D me32.hModule;
          CloseHandle(hModuleSnap);=20
          FreeLibrary(hmodule);
          <SPAN class=3Dcode-keyword>return</SPAN>;=20
      }
     =20
  } <SPAN class=3Dcode-keyword>while</SPAN>( Module32Next( hModuleSnap, =
&amp;me32 ) );=20
 =20
  CloseHandle(hModuleSnap);=20
}

