KEYEVENTF_KEYUP;

        SendInput(<SPAN class=3Dcode-digit>2</SPAN>, inp, <SPAN =
class=3Dcode-keyword>sizeof</SPAN>(INPUT));   =20
    }

    <SPAN class=3Dcode-keyword>return</SPAN> iKeyStrokeCount;
