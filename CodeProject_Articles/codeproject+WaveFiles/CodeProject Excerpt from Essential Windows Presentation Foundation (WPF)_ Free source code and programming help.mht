class=3Dcode-leadattribute>EventTrigger</SPAN> <SPAN =
class=3Dcode-attribute>RoutedEvent</SPAN><SPAN =
class=3Dcode-keyword>=3D'</SPAN><SPAN =
class=3Dcode-keyword>FrameworkElement.Loaded'</SPAN><SPAN =
class=3Dcode-keyword>&gt;</SPAN>
   <SPAN class=3Dcode-keyword>&lt;</SPAN><SPAN =
class=3Dcode-leadattribute>EventTrigger.Actions</SPAN><SPAN =
class=3Dcode-keyword>&gt;</SPAN>
    <SPAN class=3Dcode-keyword>&lt;</SPAN><SPAN =
class=3Dcode-leadattribute>BeginStoryboard</SPAN><SPAN =
class=3Dcode-keyword>&gt;</SPAN>

     <SPAN class=3Dcode-keyword>&lt;</SPAN><SPAN =
class=3Dcode-leadattribute>BeginStoryboard.Storyboard</SPAN><SPAN =
class=3Dcode-keyword>&gt;</SPAN>
      <SPAN class=3Dcode-keyword>&lt;</SPAN><SPAN =
class=3Dcode-leadattribute>Storyboard</SPAN><SPAN =
class=3Dcode-keyword>&gt;</SPAN>
<STRONG>       <SPAN class=3Dcode-keyword>&lt;</SPAN><SPAN =
class=3Dcode-leadattribute>DoubleAnimation</STRONG></SPAN>
<SPAN class=3Dcode-attribute><STRONG></SPAN>        <SPAN =
class=3Dcode-attribute>From</SPAN><SPAN =
class=3Dcode-keyword>=3D'</SPAN><SPAN =
class=3Dcode-keyword>-25'</SPAN><SPAN =
class=3Dcode-attribute></STRONG></SPAN>
<SPAN class=3Dcode-attribute><STRONG></SPAN>        <SPAN =
class=3Dcode-attribute>To</SPAN><SPAN =
class=3Dcode-keyword>=3D'</SPAN><SPAN =
class=3Dcode-keyword>25'</SPAN><SPAN =
class=3Dcode-attribute></STRONG></SPAN>

<SPAN class=3Dcode-attribute><STRONG></SPAN>        <SPAN =
class=3Dcode-attribute>Storyboard.TargetName</SPAN><SPAN =
class=3Dcode-keyword>=3D'</SPAN><SPAN =
class=3Dcode-keyword>rotation'</SPAN><SPAN =
class=3Dcode-attribute></STRONG></SPAN>
<SPAN class=3Dcode-attribute><STRONG></SPAN>        <SPAN =
class=3Dcode-attribute>Storyboard.TargetProperty</SPAN><SPAN =
class=3Dcode-keyword>=3D'</SPAN><SPAN =
class=3Dcode-keyword>Angle'</SPAN><SPAN =
class=3Dcode-attribute></STRONG></SPAN>
<SPAN class=3Dcode-attribute><STRONG></SPAN>        <SPAN =
class=3Dcode-attribute>AutoReverse</SPAN><SPAN =
