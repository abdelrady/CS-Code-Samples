class=3Dcode-keyword>return</SPAN> invert=3D=3D-1; }
        <SPAN class=3Dcode-keyword>set</SPAN> { invert =3D value ? -1 : =
<SPAN class=3Dcode-digit>1</SPAN>; }
    }

    <SPAN class=3Dcode-preprocessor>#endregion</SPAN>  [ Properties ]

