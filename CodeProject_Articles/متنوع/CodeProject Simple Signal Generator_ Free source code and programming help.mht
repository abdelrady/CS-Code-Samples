class=3Dcode-SummaryComment>&gt;</SPAN></SPAN>
    <SPAN class=3Dcode-keyword>private</SPAN> <SPAN =
class=3Dcode-keyword>long</SPAN> ticksPerSecond =3D Stopwatch.Frequency;

    <SPAN class=3Dcode-preprocessor>#endregion</SPAN>  [ Private ]

    <SPAN class=3Dcode-preprocessor>#region</SPAN> [ Public ... ]

    <SPAN class=3Dcode-keyword>public</SPAN> SignalGenerator(SignalType =
initialSignalType)
    {
        signalType =3D initialSignalType;
    }

    <SPAN class=3Dcode-keyword>public</SPAN> SignalGenerator() { }

    <SPAN class=3Dcode-preprocessor>#if</SPAN> DEBUG
    <SPAN class=3Dcode-keyword>public</SPAN> <SPAN =
class=3Dcode-keyword>float</SPAN> GetValue(<SPAN =
class=3Dcode-keyword>float</SPAN> time)
    <SPAN class=3Dcode-preprocessor>#else</SPAN>
    <SPAN class=3Dcode-keyword>private</SPAN> <SPAN =
class=3Dcode-keyword>float</SPAN> GetValue(<SPAN =
class=3Dcode-keyword>float</SPAN> time)
    <SPAN class=3Dcode-preprocessor>#endif</SPAN>
    {
        <SPAN class=3Dcode-keyword>float</SPAN> value =3D 0f;
        <SPAN class=3Dcode-keyword>float</SPAN> t =3D frequency * time + =
phase;
        <SPAN class=3Dcode-keyword>switch</SPAN> (signalType)
        { <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment> http://en.wikipedia.org/wiki/Waveform</SPAN>
            <SPAN class=3Dcode-keyword>case</SPAN> SignalType.Sine: =
<SPAN class=3Dcode-comment>//</SPAN><SPAN class=3Dcode-comment> sin( 2 * =
pi * t )</SPAN>
                value =3D (<SPAN =
class=3Dcode-keyword>float</SPAN>)Math.Sin(2f*Math.PI*t);
                <SPAN class=3Dcode-keyword>break</SPAN>;
            <SPAN class=3Dcode-keyword>case</SPAN> SignalType.Square: =
<SPAN class=3Dcode-comment>//</SPAN><SPAN class=3Dcode-comment> sign( =
sin( 2 * pi * t ) )</SPAN>
                value =3D Math.Sign(Math.Sin(2f*Math.PI*t));
                <SPAN class=3Dcode-keyword>break</SPAN>;
            <SPAN class=3Dcode-keyword>case</SPAN> SignalType.Triangle:
            <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment> 2 * abs( t - 2 * floor( t / 2 ) - 1 ) - 1</SPAN>
                value =3D 1f-4f*(<SPAN =
class=3Dcode-keyword>float</SPAN>)Math.Abs
                    ( Math.Round(t-0.25f)-(t-0.25f) );
                <SPAN class=3Dcode-keyword>break</SPAN>;
            <SPAN class=3Dcode-keyword>case</SPAN> SignalType.Sawtooth:
            <SPAN class=3Dcode-comment>//</SPAN><SPAN =
class=3Dcode-comment> 2 * ( t/a - floor( t/a + 1/2 ) )</SPAN>
                value =3D 2f*(t-(<SPAN =
class=3Dcode-keyword>float</SPAN>)Math.Floor(t+0.5f));
                <SPAN class=3Dcode-keyword>break</SPAN>;
        }

        <SPAN =
class=3Dcode-keyword>return</SPAN>(invert*amplitude*value+offset);
    }

    <SPAN class=3Dcode-keyword>public</SPAN> <SPAN =
class=3Dcode-keyword>float</SPAN> GetValue()
    {
        <SPAN class=3Dcode-keyword>float</SPAN> time =3D (<SPAN =
class=3Dcode-keyword>float</SPAN>)(Stopwatch.GetTimestamp() - startTime)
                        / ticksPerSecond;
        <SPAN class=3Dcode-keyword>return</SPAN> GetValue(time);
    }

    <SPAN class=3Dcode-keyword>public</SPAN> <SPAN =
class=3Dcode-keyword>void</SPAN> Reset()
    {
        startTime =3D Stopwatch.GetTimestamp();
    }

