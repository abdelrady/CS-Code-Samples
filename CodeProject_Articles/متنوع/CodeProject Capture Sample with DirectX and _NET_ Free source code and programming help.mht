        <SPAN class=3Dcode-keyword>if</SPAN> ( hr !=3D <SPAN =
class=3Dcode-digit>0</SPAN> ) Marshal.ThrowExceptionForHR( hr );=20

        firstFrame =3D <SPAN class=3Dcode-keyword>false</SPAN>;
    }

