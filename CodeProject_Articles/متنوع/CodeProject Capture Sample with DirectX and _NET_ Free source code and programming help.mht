        Marshal.ThrowExceptionForHR( hr );

    isPreviewRendered =3D <SPAN class=3Dcode-keyword>true</SPAN>;
    didSomething =3D <SPAN class=3Dcode-keyword>true</SPAN>;

    <SPAN class=3Dcode-comment>//</SPAN><SPAN class=3Dcode-comment> =
Begin Configuration of SampGrabber    =
