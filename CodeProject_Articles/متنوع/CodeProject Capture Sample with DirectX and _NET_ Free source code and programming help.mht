</SPAN>    hr =3D videoWindow.put_Visible( DsHlp.OATRUE );
    <SPAN class=3Dcode-keyword>if</SPAN>( hr &lt; <SPAN =
class=3Dcode-digit>0</SPAN> ) Marshal.ThrowExceptionForHR( hr );

    hr =3D mediaEvt.SetNotifyWindow( <SPAN =
