      <P><STRONG>Third</STRONG>, Add a HttpModule class </P><PRE =
lang=3DCS><SPAN class=3Dcode-keyword>public</SPAN> <SPAN =
class=3Dcode-keyword>class</SPAN> DoubleLoginMonitorModule : IHttpModule
{

	<SPAN class=3Dcode-keyword>public</SPAN> <SPAN =
class=3Dcode-SDKkeyword>String</SPAN> ModuleName
	{
	    <SPAN class=3Dcode-keyword>get</SPAN> { <SPAN =
class=3Dcode-keyword>return</SPAN> <SPAN =
class=3Dcode-string>"</SPAN><SPAN =
class=3Dcode-string>DoubleLoginMonitorModule"</SPAN>; }
	}

	<SPAN class=3Dcode-keyword>public</SPAN> <SPAN =
class=3Dcode-keyword>void</SPAN> Init(HttpApplication application)
	{
	    application.PreRequestHandlerExecute +=3D <SPAN =
class=3Dcode-keyword>new</SPAN> =
EventHandler(Application_PreRequestHandlerExecute);

	}

	<SPAN class=3Dcode-keyword>private</SPAN> <SPAN =
class=3Dcode-keyword>void</SPAN> =
Application_PreRequestHandlerExecute(<SPAN =
class=3Dcode-SDKkeyword>Object</SPAN> source, EventArgs e)
	{
	    HttpApplication application =3D (HttpApplication)source;
	    HttpContext context =3D application.Context;

	    <SPAN class=3Dcode-comment>//</SPAN><SPAN class=3Dcode-comment> if =
double login, kick it out</SPAN>
	    <SPAN class=3Dcode-keyword>if</SPAN> (LoginState.IsDoubleLogin())
	    {
		HttpContext.Current.Response.Redirect(<SPAN =
class=3Dcode-string>"</SPAN><SPAN =
class=3Dcode-string>logoff.htm"</SPAN>);
		HttpContext.Current.Response.End();
	    }
	}

	<SPAN class=3Dcode-keyword>public</SPAN> <SPAN =
class=3Dcode-keyword>void</SPAN> Dispose()
	{
	}
}
</PRE>
      <P>The <CODE>LoginState.IsDoubleLogin()</CODE> is detailed =
here</P><PRE lang=3DCS><SPAN class=3Dcode-SummaryComment>///</SPAN><SPAN =
class=3Dcode-comment> <SPAN class=3Dcode-SummaryComment>&lt;</SPAN><SPAN =
class=3Dcode-SummaryComment>summary</SPAN><SPAN =
class=3Dcode-SummaryComment>&gt;</SPAN></SPAN>
<SPAN class=3Dcode-SummaryComment>///</SPAN><SPAN class=3Dcode-comment> =
